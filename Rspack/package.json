{
  "name": "Rspack",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "rspack": "react-native webpack-start",
    "rspack:mini": "react-native webpack-start --port 9001",
    "test": "jest",
    "pod": "cd ios && pod install && cd ..",
    "clean": "rm -rf ios/Podfile.lock && rm -rf ios/Pods/",
    "webpack_android": "react-native webpack-start --host 127.0.0.1",
    "bundle": "yarn bundle:ios && yarn bundle:android",
    "bundle:ios": "react-native webpack-bundle --platform ios --entry-file index.js --dev false",
    "bundle:android": "react-native webpack-bundle --platform android --entry-file index.js --dev false",
    "analyze": "RSDOCTOR=true react-native webpack-bundle --platform android --entry-file index.js --dev false",
    "setup": "node ./scripts/setup.js"
  },
